{
  "name": "Page Redder",
  "version": "2.0",
  "permissions": [
    "activeTab"
    ],
    "background": {
      "scripts": ["jquery.js", "background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_title": "Make this page red"
    },
    "manifest_version": 2,
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    }
  }
