{
  "name": "Sarah Test",
  "manifest_version": 2,
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["jquery.js", "get_number.js"]
    }
  ],
  "permissions": [
     "tabs"
  ]
}
